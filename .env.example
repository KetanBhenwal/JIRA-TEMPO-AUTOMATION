# Copy this file to .env and fill in values
JIRA_BASE_URL=https://your-domain.atlassian.net/
TEMPO_BASE_URL=https://api.tempo.io/4
JIRA_EMAIL=you@example.com
JIRA_API_TOKEN=atlassian_api_token_here
TEMPO_API_TOKEN=tempo_api_token_here
TEMPO_ACCOUNT_ID=your_atlassian_account_id
PORT=3000

# Optional AI agent tuning
AI_AGENT_TEST_MODE=false
AI_AGENT_DRY_RUN=false
AI_AGENT_VERBOSE_LOG=false
AI_AGENT_MEETING_SCORE_THRESHOLD=7
AI_AGENT_RUNNING_APPS_REFRESH_MS=300000
AI_AGENT_DEFAULT_MEETING_ISSUE=YOUR_MEETING_ISSUE_KEY_HERE
